#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int n;
    scanf("%d", &n);
    
    int current_value = 7; 
    
    for (int i = 1; i <= n; i++) {
        printf("%d ", current_value);
        if (i % 2 != 0) {
            current_value -= 2;
        } else {
            current_value += 3;
        }
    }

    return 0;
}


