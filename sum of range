#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int start, end, sum = 0;
    
    
    
    scanf("%d\n", &start);
    
    scanf("%d\n", &end);
    
    
    for (int i = start; i <= end; i++) {
        sum += i;
    }
    
    
    printf("%d\n", sum);


    return 0;
}
